EX:Bar RDF:type OWL:Class ;
       OWL:equivalentClass [ OWL:intersectionOf ( SDO:LocationFeatureSpecification
                                                  [ RDF:type OWL:Restriction ;
                                                    OWL:onProperty SDO:value ;
                                                    OWL:hasValue "Bar / Lounge"@en
                                                  ]
                                                ) ;
                             RDF:type OWL:Class
                           ] .


